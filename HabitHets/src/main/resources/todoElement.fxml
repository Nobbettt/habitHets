         <graphic>
            <ImageView fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/delete-icon1c.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
