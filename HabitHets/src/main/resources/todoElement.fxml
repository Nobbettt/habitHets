<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="40.0" prefWidth="190.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <CheckBox layoutX="3.0" layoutY="11.0" mnemonicParsing="false" onAction="#remove" prefHeight="18.0" prefWidth="90.0" text="CheckBox" />
      <Button fx:id="deleteTodo" layoutX="149.0" layoutY="7.0" mnemonicParsing="false" onAction="#deleteTodo" prefHeight="20.0" prefWidth="20.0">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/baseline_delete_forever_black_36dp.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
